{
  "version": "10.2.1",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "allowBranch": [
        "master"
      ],
      "conventionalCommits": true,
      "changelogPreset": "angular",
      "createRelease": "github",
      "message": "chore(release): publish %v",
      "npmClient": "yarn"
    }
  },
  "ignoreChanges": [
    "**/example/**",
    "specification/**",
    "**/*.spec.js",
    "**/*.tgz",
    "**/^(?!README).md"
  ]
}
